{
  "name": "excel-database-electron",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "vite",
    "renderer:dev": "cd src/renderer-app && vite",
    "renderer:build": "cd src/renderer-app && vite build",
    "start:dev": "RENDERER_DEV_URL=http://localhost:5173 concurrently \"npm:renderer:dev\" \"electron .\"",
    "start:prod": "electron .",
    "build": "electron-builder",
    "cleanup": "rm -rf node_modules src/renderer-app/node_modules src/renderer-app/dist archived_components/legacy_renderer/ui || true"
  },
  "dependencies": {
    "electron": "^31.0.0",
    "node-fetch": "^3.3.2",
    "proper-lockfile": "^4.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.0",
    "concurrently": "^8.0.0",
    "electron-builder": "^23.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
